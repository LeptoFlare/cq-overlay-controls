{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "minItems": 2,
  "maxItems": 2,
  "items": {
    "type": "object",
    "properties": {
      "name": { "type": "string" },
      "roster": {
        "type": "array",
        "items": { "type": "string" }
      }
    }
  },
  "default": [
    {
      "name": "Team A",
      "roster": ["Player A1", "Player A2", "Player A3", "Player A4"]
    },
    {
      "name": "Team B",
      "roster": ["Player B1", "Player B2", "Player B3", "Player B4"]
    }
  ]
}
